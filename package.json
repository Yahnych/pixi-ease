{
    "name": "pixi-ease",
    "version": "0.16.0",
    "description": "pixi.js animation library using easing functions",
    "main": "dist/index.js",
    "scripts": {
        "test": "budo docs/code.js:index.js --live --dir docs",
        "build-demo": "browserify docs/code.js -o docs/index.js",
        "transpile": "./node_modules/.bin/babel src/ --out-dir dist --source-maps inline",
        "docs": "jsdoc -c docs/jsdoc.conf.json",
        "prepublishOnly": "npm run transpile && npm run build-demo && npm run docs"
    },
    "repository": {
        "type": "git",
