    "bugs": {
        "url": "https://github.com/davidfig/pixi-ease/issues"
    },
    "homepage": "https://github.com/davidfig/pixi-ease#readme",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "eventemitter3": "^3.0.0",
        "penner": "^0.1.3",
        "yy-angle": "^1.2.0",
        "yy-color": "^1.0.0"
    },
    "peerDependencies": {
        "pixi.js": ">=4.6.0"
    },
    "devDependencies": {
        "fork-me-github": "^1.0.3",
        "highlight.js": "^9.12.0",
        "pixi.js": "^4.6.2",
